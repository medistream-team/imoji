{
  "name": "imoji-editor",
  "version": "0.1.4",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "description": "JavaScript image editor with sticker feature",
  "keywords": [
    "vuejs",
    "vue",
    "javascript",
    "component",
    "image-editor",
    "editor",
    "image",
    "crop",
    "move",
    "zoom",
    "rotate",
    "sticker",
    "front-end"
  ],
  "files": [
    "dist/imoji-editor.umd.min.js",
    "dist/img/*.*"
  ],
  "main": "dist/imoji-editor.umd.min.js",
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "cropperjs": "^1.5.11",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^6.2.2",
    "fabric": "^4.5.0",
    "vue-material-design-icons": "^4.12.1",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:vue/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": true,
          "tabWidth": 2,
          "arrowParens": "avoid",
          "endOfLine": "lf"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
